+    #address-cells = <1>;
+    #size-cells = <0>;
+
+    status = "okay";
+    pinctrl-names = "default";
+    pinctrl-0 = <&spi0_pins &spi0_cs_pins>;
+    cs-gpios = <&pio 2 3 GPIO_ACTIVE_HIGH>, <&pio 0 6 GPIO_ACTIVE_HIGH>; 
+
+    spidev0: spi@0 {
+        compatible = "spidev";
+        reg = <0>;
+        spi-max-frequency = <10000000>;
+        status = "okay";
+    };
+};*/
+
